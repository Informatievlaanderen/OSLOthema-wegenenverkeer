@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType> a skos:ConceptScheme;
  skos:definition "Types van boombrug."@nl;
  skos:prefLabel "Boombrug type"@nl.

<https://wegenenverkeer.data.vlaanderen.be/id/concept/KlEcoBoombrugType/touwbrug> a skos:Concept;
  skos:definition "Een boombrug bestaande uit een gedraaid touw van natuurlijke of kunstmatige vezels, dat strak over de weg wordt gespannen en voldoende dik is om er dieren op een stabiele wijze overheen te laten lopen."@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType>;
  skos:notation "touwbrug";
  skos:prefLabel "touwbrug"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/KlEcoBoombrugType/ladderbrug> a skos:Concept;
  skos:definition "Een boombrug waarbij de oversteek bestaat uit een laddervorm gemaakt uit touw (touwladder) of metaal-kunststof (kabelnet) of een weefsel van takken en nylon (Takkenmat)."@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType>;
  skos:notation "ladderbrug";
  skos:prefLabel "ladderbrug"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/KlEcoBoombrugType/tunnelbrug> a skos:Concept;
  skos:definition "Een boombrug bestaande uit een tunnelvormige oversteek gemaakt uit geweven touwen (touwtunnel) of een buisvormige draadkoker (kokerbrug)"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType>;
  skos:notation "tunnelbrug";
  skos:prefLabel "tunnelbrug"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/KlEcoBoombrugType/portaal-boomgoot> a skos:Concept;
  skos:definition "Een bestaande portaal voor signalisatie ingericht als boomrug door middel van een aluminium ladder of houten loopplank of goot."@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType>;
  skos:notation "portaal-boomgoot";
  skos:prefLabel "portaal boomgoot"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/KlEcoBoombrugType> .
