@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> a skos:ConceptScheme;
  skos:definition "Keuzelijst van verschillende mogelijke conflicten bij een boomvorm"@nl;
  skos:prefLabel "BoomConflicten"@nl.

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Hinderende%20takken> a skos:Concept;
  skos:definition "Takken die hinderlijk zijn voor de zichtbaarheid en de vrije doorrijhoogte of die luchtleidingen raken"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Hinderende takken";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Wortelopdruk> a skos:Concept;
  skos:definition "Boomwortels die (weg)verhardingselementen opdrukken"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Wortelopdruk";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Verharding> a skos:Concept;
  skos:definition "Een verhardingslaag bevindt zich zeer dicht tegen de boom"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Verharding";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Bodemverdichting> a skos:Concept;
  skos:definition "Verdichting van de bodem door belasting (voetgangers, fietsers, auto's, bouwmaterialen,...)"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Bodemverdichting";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Te%20dicht%20bij%20rand%20verharding> a skos:Concept;
  skos:definition "Verharding ligt op minder dan 50 cm van de stamvoet"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Te dicht bij rand verharding";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Beperkte%20doorwortelbare%20ruimte> a skos:Concept;
  skos:definition "Het bodemvolume met voldoende mineralen, water en zuurstof waar de wortels van de boom zich kunnen ontwikkelen is veel lager dan verwacht"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Beperkte doorwortelbare ruimte";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Scheefstand> a skos:Concept;
  skos:definition "Schuin gezakte/gegroeide boom waarbij de top van de boom in het verlengde van de stam ligt en niet vertikaal opgericht is"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Scheefstand";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Instabiel> a skos:Concept;
  skos:definition "Een boom die niet stabiel in de grond staat"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Instabiel";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .

<https://wegenenverkeer.data.vlaanderen.be/id/concept/BoomConflicten/Andere> a skos:Concept;
  skos:definition "Andere mogelijke conflicten"@nl;
  skos:inScheme <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten>;
  skos:notation "Andere";
  skos:prefLabel "BoomConflicten"@nl;
  skos:topConceptOf <https://wegenenverkeer.data.vlaanderen.be/id/conceptscheme/BoomConflicten> .
